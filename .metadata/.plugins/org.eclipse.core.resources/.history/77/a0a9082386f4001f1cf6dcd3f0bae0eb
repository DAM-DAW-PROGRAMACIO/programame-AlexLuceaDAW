package Easy;

import java.util.Scanner;

public class p190 {

	public static void main(String[] args) throws Exception{
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		
		long num=sc.nextInt();
		long den = sc.nextInt();
		long magicNum;
		long resultat=1;
		
		while(!(num<den)) {
			
			magicNum = num-den;
			
			for (long i= 1; i<=magicNum; i++) {
				resultat *=i;
			}
			
			System.out.println(resultat);
			num=sc.nextInt();
			den = sc.nextInt();
		}
	}

}
